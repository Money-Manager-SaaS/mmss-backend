{
  "name": "mmss-backend",
  "version": "0.0.1",
  "description": "Money Manager Cloud back-end, to handle CRUD and Authentication and keep the compatibility to the Money Manager EX",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"ts-node src/server.ts\"",
    "start": "npx pm2 start pm2.yaml",
    "postinstall": "npx tsc",
    "test": "jest"
  },
  "dependencies": {
    "@types/bookshelf": "^1.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.7",
    "@types/request-promise": "^4.1.45",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^4.1.1",
    "bookshelf": "^1.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^5.0.0-alpha.7",
    "knex": "^0.20.10",
    "pg": "^7.18.2",
    "pm2": "^4.2.3",
    "pug": "^2.0.4",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "sqlite3": "^4.1.1",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/jest": "25.1.3",
    "jest": "25.1.0",
    "ts-jest": "25.2.1",
    "tsc-watch": "4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Money-Manager-SaaS/mmss-backend.git"
  },
  "keywords": [
    "MMSS",
    "Money",
    "Manager"
  ],
  "author": "MMSS-Admin",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Money-Manager-SaaS/mmss-backend/issues"
  },
  "homepage": "https://github.com/Money-Manager-SaaS/mmss-backend#readme",
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
